{
  "servers": {
    "io.github.upstash/context7": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@upstash/context7-mcp@1.0.30"
      ],
      "env": {
        "CONTEXT7_API_KEY": "${input:CONTEXT7_API_KEY}"
      },
      "gallery": "https://api.mcp.github.com",
      "version": "1.0.30"
    },
    "io.github.ChromeDevTools/chrome-devtools-mcp": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "chrome-devtools-mcp@0.10.2"
      ],
      "gallery": "https://api.mcp.github.com",
      "version": "0.10.2"
    },
    "antfu/nuxt-mcp": {
      "type": "http",
      "url": "https://mcp.nuxt.com/sse",
      "gallery": "https://api.mcp.github.com",
      "version": "1.0.0"
    },
    "microsoft/playwright-mcp": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ],
      "gallery": "https://api.mcp.github.com",
      "version": "0.0.1-seed"
    }
  },
  "inputs": [
    {
      "id": "CONTEXT7_API_KEY",
      "type": "promptString",
      "description": "API key for authentication",
      "password": true
    }
  ]
}
