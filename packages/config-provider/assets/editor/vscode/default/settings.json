{
  // cSpell:disable
  /* --------------------------------- Visual --------------------------------- */
  // Theme
  "workbench.preferredDarkColorTheme": "Vitesse Dark",
  "workbench.preferredLightColorTheme": "Vitesse Light",
  "workbench.productIconTheme": "icons-carbon",
  "workbench.iconTheme": "catppuccin-mocha",
  // Window
  "window.autoDetectColorScheme": true,
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 0.5,
  // Workbench
  "workbench.activityBar.orientation": "vertical",
  "workbench.tree.renderIndentGuides": "always",
  "workbench.tree.indent": 16,
  // Editor
  "editor.guides.bracketPairs": true,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [80, 120, 160, 200],
  "editor.tokenColorCustomizations": {
    "[*Light*]": {
      "textMateRules": []
    },
    "[*Dark*]": {
      "textMateRules": []
    },
    "textMateRules": []
  },
  "editor.wordWrap": "on",
  "editor.wrapOnEscapedLineFeeds": true,
  "scm.diffDecorationsGutterWidth": 2,
  // Font
  "custom-ui-style.font.monospace": "'Monaspace Xenon', 'Maple Mono CN', 'Symbols Nerd Font Mono', monospace",
  "custom-ui-style.font.sansSerif": "'Monaspace Xenon', 'Maple Mono CN', 'Symbols Nerd Font Mono', sans-serif",
  "editor.fontFamily": "'Monaspace Xenon', 'Maple Mono CN', 'Symbols Nerd Font Mono', monospace",
  "editor.fontSize": 16,
  "editor.lineHeight": 1.5,
  "editor.fontLigatures": true,
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontLigatures.enabled": true,
  "markdown.preview.fontFamily": "'Monaspace Xenon', 'Maple Mono CN', 'Symbols Nerd Font Mono', monospace",
  "markdown.preview.fontSize": 14,
  // Cursor
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": "on",
  // Highlight
  "highlight-matching-tag.styles": {
    "opening": { "name": { "highlight": "rgb(143 188 143 / 40%)" } }
  },
  "todohighlight.include": [
    "**/.*", // Include all dot files
    "**/*" // Include all non-dot files
  ],
  "todohighlight.exclude": [
    "**/node_modules/**",
    "**/bower_components/**",
    "**/dist/**",
    "**/build/**",
    "**/.vscode/**",
    "**/.vscode-test/**",
    "**/.github/**",
    "**/_output/**",
    "**/*.min.*",
    "**/*.map",
    "**/.next/**",
    "**/.output/**"
  ],
  "todohighlight.defaultStyle": {
    "color": "#fff",
    "borderRadius": "4px"
  },
  "todohighlight.keywords": [
    {
      "text": "BUG(string):",
      "regex": { "pattern": "(?<=^|\"|\\s)(BUG|CAUTION)(\\(\\w+\\))?:" },
      "backgroundColor": "#ab372f",
      "overviewRulerColor": "rgb(171 55 47 / 80%)"
    },
    {
      "text": "FIXME(string):",
      "regex": { "pattern": "(?<=^|\"|\\s)(FIXME|SHAMELESSLY DISABLED)(\\(\\w+\\))?:" },
      "backgroundColor": "#e18a3b",
      "overviewRulerColor": "rgb(225 138 59 / 80%)"
    },
    {
      "text": "TODO(string):",
      "regex": { "pattern": "(?<=^|\"|\\s)(TODO)(\\(\\w+\\))?:" },
      "backgroundColor": "#3271ae",
      "overviewRulerColor": "rgb(50 113 174 / 80%)"
    },
    {
      "text": "[ ]",
      "backgroundColor": "#3271ae",
      "overviewRulerColor": "rgb(50 113 174 / 80%)"
    },
    {
      "text": "WIP(string):",
      "regex": { "pattern": "(?<=^|\"|\\s)(WIP)(\\(\\w+\\))?:" },
      "backgroundColor": "#6ca8af",
      "overviewRulerColor": "rgb(108 168 175 / 80%)"
    },
    {
      "text": "[~]",
      "backgroundColor": "#6ca8af",
      "overviewRulerColor": "rgb(108 168 175 / 80%)"
    },
    {
      "text": "DONE(string):",
      "regex": { "pattern": "(?<=^|\"|\\s)(DONE|RECOMMENDED)(\\(\\w+\\))?:" },
      "backgroundColor": "#68945c",
      "overviewRulerColor": "rgb(104 148 92 / 80%)"
    },
    {
      "text": "[x]",
      "backgroundColor": "#68945c",
      "overviewRulerColor": "rgb(104 148 92 / 80%)"
    },
    {
      "text": "NOTE(string):",
      "regex": { "pattern": "(?<=^|\"|\\s)(NOTE|NOTICE)(\\(\\w+\\))?:" },
      "backgroundColor": "#cf929e",
      "overviewRulerColor": "rgb(207 146 158 / 80%)"
    },
    {
      "text": "OPTIONAL(string):",
      "regex": { "pattern": "(?<=^|\"|\\s)(OPTIONAL)(\\(\\w+\\))?:" },
      "backgroundColor": "#9aa7b1",
      "overviewRulerColor": "rgb(154 167 177 / 80%)"
    },
    {
      "text": "<NOT OPENVSX>",
      "backgroundColor": "#7e527f",
      "overviewRulerColor": "rgb(126 82 127 / 80%)"
    }
  ],

  /* -------------------------------- Behavior -------------------------------- */
  // Security
  "security.workspace.trust.enabled": false,
  // Profile
  "window.newWindowProfile": "Default",
  // File Associations
  "files.associations": {
    ".env*": "dotenv",
    ".env*.js": "javascript",
    ".env*.ts": "typescript",
    ".simple-git-hooks.rc": "shellscript",
    ".czrc": "json"
  },
  // Prompts
  "window.dialogStyle": "custom",
  "files.simpleDialog.enable": true,
  // Status Bar
  "selectline.alignment": "right",
  "selectline.displayFormat": "%d lines selected",
  "selectline.statusbarPriority": 100.4,
  // Explorer
  "explorer.autoOpenDroppedFile": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  // Search
  "search.showLineNumbers": true,
  "search.exclude": {
    "**/*.snap": true,
    "**/*.svg": true,
    "**/.git": true,
    "**/.github": false,
    "**/.nuxt": true,
    "**/.output": true,
    "**/.pnpm": true,
    "**/.vscode": true,
    "**/.yarn": true,
    "**/assets": true,
    "**/bower_components": true,
    "**/dist/**": true,
    "**/logs": true,
    "**/node_modules": true,
    "**/out/**": true,
    "**/package-lock.json": true,
    "**/pnpm-lock.yaml": true,
    "**/public": true,
    "**/temp": true,
    "**/yarn.lock": true,
    "**/CHANGELOG*": true,
    "**/LICENSE*": true
  },
  // Git
  "git.autofetch": true,
  "git.confirmSync": true,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "always",
  "git.untrackedChanges": "separate",
  "git.blame.editorDecoration.enabled": true,
  "git.blame.statusBarItem.enabled": true,
  // Editor Group
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.editor.limit.value": 5,
  "workbench.startupEditor": "none",
  // Editor
  "editor.minimap.enabled": true,
  "editor.accessibilitySupport": "off",
  "editor.unusualLineTerminators": "auto",
  // Diff Editor
  "diffEditor.ignoreTrimWhitespace": false,
  // Terminal
  "terminal.integrated.env.windows": {},
  "terminal.integrated.profiles.linux": {
    "nushell": { "path": "nu" }
  },
  "terminal.integrated.profiles.osx": {
    "nushell": { "path": "nu" }
  },
  "terminal.integrated.profiles.windows": {
    "Nushell": { "path": "C:\\Program Files\\nu\\bin\\nu.exe" }
  },
  "terminal.integrated.defaultProfile.linux": "nushell",
  "terminal.integrated.defaultProfile.osx": "nushell",
  "terminal.integrated.defaultProfile.windows": "Nushell",
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "terminal.integrated.rightClickBehavior": "default",
  // Inlay Hints
  "arrayIndexInlay.minLength": 5,
  "javascript.inlayHints.parameterNames.enabled": "all",
  "javascript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.inlayHints.parameterTypes.enabled": true,
  "vue.inlayHints.destructuredProps": true,
  "vue.inlayHints.inlineHandlerLeading": true,
  "vue.inlayHints.missingProps": true,
  "vue.inlayHints.optionsWrapper": true,
  // Lens
  "errorLens.enabledDiagnosticLevels": ["warning", "error"],
  "errorLens.messageTemplate": "$severity$message",
  "errorLens.severityText": ["■ ", "■ ", "■ ", "■ "],
  // Suggestion & Completion
  "css.enabledLanguages": ["html", "vue"],
  "css.styleSheets": ["**/*.{c,sc,sa,le,postc}ss"],
  // Emmet
  "emmet.preferences": {
    "bem.modifierSeparator": "--"
  },
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.syntaxProfiles": {
    "html": { "filters": "bem" }
  },
  "emmet.triggerExpansionOnTab": false,
  // Scroll
  "workbench.list.mouseWheelScrollSensitivity": 2,
  "workbench.list.fastScrollSensitivity": 3,
  "workbench.list.smoothScrolling": true,
  "editor.mouseWheelScrollSensitivity": 2,
  "editor.fastScrollSensitivity": 3,
  "editor.smoothScrolling": true,
  "terminal.integrated.mouseWheelScrollSensitivity": 2,
  "terminal.integrated.fastScrollSensitivity": 3,
  "terminal.integrated.smoothScrolling": true,
  // Zoom
  "editor.mouseWheelZoom": true,
  // Goto
  "editor.gotoLocation.multipleDefinitions": "goto",
  // AI
  "cursor.cpp.disabledLanguages": ["plaintext", "dotenv"],
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": true,
    "scminput": false,
    "dotenv": false
  },
  "github.copilot.nextEditSuggestions.enabled": true,
  "chat.mcp.gallery.enabled": true,
  "chat.mcp.autostart": "never",

  /* --------------------------------- Format --------------------------------- */
  "editor.tabSize": 2,
  "files.insertFinalNewline": true,
  "editor.detectIndentation": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": false,
  "editor.formatOnPaste": false,
  "editor.formatOnType": false,
  "shellformat.useEditorConfig": true,

  /* ---------------------------------- Lint ---------------------------------- */
  "cSpell.allowCompoundWords": true,
  "cSpell.customDictionaries": {
    "common": { "name": "common", "path": "~/.cspell.common.txt", "description": "Words used in common situation.", "addWords": false },
    "work": { "name": "work", "path": "~/.cspell.work.txt", "description": "Words used in work.", "addWords": false }
  },
  "cSpell.ignorePaths": [
    // all of dotfiles & dotfolders
    ".*",
    "dist",
    "node_modules",
    "package.json",
    "package-lock.json",
    "yarn.lock",
    "pnpm-lock.yaml",
    "pnpm-workspace.yaml",
    "bun.lockb",
    "bun.lock"
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue",
    "html",
    "markdown",
    "json",
    "jsonc",
    "yaml",
    "toml",
    "xml",
    "css",
    "postcss",
    "scss",
    "sass",
    "less"
  ],
  "eslint.rules.customizations": [
    { "rule": "style/*", "severity": "off" },
    { "rule": "format/*", "severity": "off" },
    { "rule": "*-indent", "severity": "off" },
    { "rule": "*-spacing", "severity": "off" },
    { "rule": "*-spaces", "severity": "off" },
    { "rule": "*-order", "severity": "off" },
    { "rule": "*-dangle", "severity": "off" },
    { "rule": "*-newline", "severity": "off" },
    { "rule": "*quotes", "severity": "off" },
    { "rule": "*semi", "severity": "off" }
  ],

