{
  "name": "starship-butler-utils",
  "type": "module",
  "version": "1.0.3",
  "description": "Utils used by starship butler.",
  "author": "Lumirelle <shabbyacc@outlook.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/lumirelle",
  "homepage": "https://github.com/lumirelle/starship-butler#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lumirelle/starship-butler.git"
  },
  "bugs": "https://github.com/lumirelle/starship-butler/issues",
  "keywords": [
    "config-butler",
    "config-manager",
    "starship-butler"
  ],
  "sideEffects": false,
  "exports": {
    "./config": {
      "import": {
        "types": "./dist/config.d.ts",
        "default": "./dist/config.js"
      }
    },
    "./path": {
      "import": {
        "types": "./dist/path.d.ts",
        "default": "./dist/path.js"
      }
    },
    "./fs": {
      "import": {
        "types": "./dist/fs.d.ts",
        "default": "./dist/fs.js"
      }
    },
    "./highlight": {
      "import": {
        "types": "./dist/highlight.d.ts",
        "default": "./dist/highlight.js"
      }
    },
    "./consola": {
      "import": {
        "types": "./dist/consola.d.ts",
        "default": "./dist/consola.js"
      }
    },
    "./prompts": {
      "import": {
        "types": "./dist/prompts.d.ts",
        "default": "./dist/prompts.js"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "bunup --watch",
    "build": "bunup",
    "distcheck": "bun attw --pack --profile esm-only && bun publint"
  },
  "dependencies": {
    "@clack/prompts": "catalog:prod",
    "ansis": "catalog:prod",
    "c12": "catalog:prod",
    "consola": "catalog:prod",
    "pathe": "catalog:prod",
    "rc9": "catalog:prod"
  }
}
